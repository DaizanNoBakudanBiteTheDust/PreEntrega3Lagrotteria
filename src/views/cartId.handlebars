<div style="display:flex; gap: 5rem;">
    <div>
        <h1 id="ola">Carrito</h1>
        <div id="container">
            <div>
                <ul>
                    {{#each cartProducts}}
                    <ul>
                        <li>Producto: {{this.product}}</li>
                        <li>Cantidad: {{this.quantity}}</li>
                    </ul>
                    {{/each}}
                    <p>CartID = {{cartId}}</p>
                     <button onclick="purchaseCart({{cartId}})">Comprar</button>
                </ul>
            </div>
        </div>
    </div>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/cartView.js"></script>
<script src="/js/purchaseCart.js"></script>
